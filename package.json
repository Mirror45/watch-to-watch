    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "axios": "^1.7.9",
    "chart.js": "^4.4.8",
    "flatpickr": "^4.6.13",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
